/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine"
    ]
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/app/app.config.ts",
    "src/app/app.routes.ts",
    "src/environments/**/*.ts",
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
